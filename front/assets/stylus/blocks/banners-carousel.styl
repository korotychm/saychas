.banners-carousel
  img
    width 100%
    height auto
  .slick-list
    margin 0 -15px
    border-radius 20px
    &::before
    &::after
      content ''
      width 15px
      height 100%
      display block
      top 0
      left 0
      position absolute
      background #fff
      z-index 2
    &::after
      left auto
      right 0
  .slick-slide
    margin 0 15px
